@prefix owl:   	<http://www.w3.org/2002/07/owl#> .					# Web Ontology Language
@prefix rdf:   	<http://www.w3.org/1999/02/22-rdf-syntax-ns#> .		# Resource Description Framework
@prefix rdfs:  	<http://www.w3.org/2000/01/rdf-schema#> .			# RDF Schema




OF:LivingRoom

bop:hasDesignAssessment OF:LivingRoomThermalComfortDesignAssessment;
bop:hasOperationalAssessment OF:LivingRoomThermalComfortOperationalAssessment.



OF:LivingRoomThermalComfortDesignAssessment

bop:hasSimpleResult "A";
bop:hasSimple95PercentPPD "5";
bop:hasSimple95PercentPMV "0.1";
bop:hasSimple80PercentPPD "3";
bop:hasSimple80PercentPMV "0.08".

OF:LivingRoomThermalComfortOperationalAssessment

bop:hasSimpleResult "B";
bop:hasSimple95PercentPPD "7";
bop:hasSimple95PercentPMV "0.25";
bop:hasSimple80PercentPPD "5";
bop:hasSimple80PercentPMV "0.15".



